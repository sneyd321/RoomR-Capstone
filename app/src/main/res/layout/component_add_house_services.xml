<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView18"
        style="@style/title_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="Services Offered"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout7" />

    <TableLayout
        android:id="@+id/tableLayout8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:animateLayoutChanges="true"
        android:stretchColumns="*"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18">

        <TableRow>

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="Key Deposit" />

            <Switch
                android:id="@+id/swtAddAgreementKeyDeposit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow>

            <LinearLayout

                android:id="@+id/llAddAgreementKeyDepositDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">


                <TextView
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Deposit Amount (Max $20)" />

                <SeekBar
                    android:id="@+id/skbAddAgreementKeyDeposit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:max="20"
                    android:padding="8dp" />

                <TextView
                    android:id="@+id/txtAddAgreementSkbKeyDepositOutput"
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="0" />
            </LinearLayout>
        </TableRow>

        <TableRow>

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Air Conditioning" />

            <Switch
                android:id="@+id/swtAddAgreementAirConditioning"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />
        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="On-Site Laundry" />

            <Switch
                android:id="@+id/swtAddAgreementLaundry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow>

            <LinearLayout

                android:id="@+id/llAddAgreementLaundryDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">


                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RadioButton
                        android:id="@+id/rbtnAddAgreementPayPerUse"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Pay Per Use" />

                    <RadioButton
                        android:id="@+id/rbtnAddAgreementIncludedInRent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Included in Rent" />
                </RadioGroup>
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Storage" />

            <Switch
                android:id="@+id/swtAddAgreementStorage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow

            android:layout_height="match_parent">

            <LinearLayout

                android:id="@+id/llAddAgreementStorageDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">


                <TextView
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Storage Amount (Max $50)" />

                <SeekBar
                    android:id="@+id/skbAddAgreementStorage"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:max="50"
                    android:padding="8dp" />

                <TextView
                    android:id="@+id/txtAddAgreementSkbStorageOutput"
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="0" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Parking" />

            <Switch
                android:id="@+id/swtAddAgreementGuestParking"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow>

            <LinearLayout

                android:id="@+id/llAddAgreementParkingDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">

                <CheckBox
                    android:id="@+id/checkBox3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Allow Guest Parking" />


                <TextView
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Parking Amount (Max $50)" />

                <SeekBar
                    android:id="@+id/skbAddAgreementParking"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:max="50"
                    android:padding="8dp" />

                <TextView
                    android:id="@+id/txtAddAgreementSkbParkingOutput"
                    style="@style/general_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="0" />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Furnished" />

            <Switch
                android:id="@+id/swtAddAgreementFurnished"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Available Outdoor Space" />

            <Switch
                android:id="@+id/swtAddAgreementAvailableOutdoorSpace"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow>

            <LinearLayout

                android:id="@+id/llAddAgreementOutdoorDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">

                <CheckBox
                    android:id="@+id/checkBox4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Backyard" />

                <CheckBox
                    android:id="@+id/checkBox5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Belcony" />

                <CheckBox
                    android:id="@+id/checkBox6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Deck" />


            </LinearLayout>
        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Smoking" />

            <Switch
                android:id="@+id/swtAddAgreementSmoking"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Tenant Insurance" />

            <Switch
                android:id="@+id/swtAddAgreementTenantInsurance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                style="@style/general_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="left"
                android:text="Appliances" />

            <Switch
                android:id="@+id/swtAddAgreementAppliances"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" />

        </TableRow>

        <TableRow>

            <LinearLayout

                android:id="@+id/llAddAgreementApplianceDrawer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/drawer_layout"
                android:orientation="vertical"
                android:visibility="gone">

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Oven" />

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Microwave" />

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Toaster" />

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Refrigerator" />

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Dishwasher" />

                <CheckBox
                    android:id="@+id/chkAddAgreementAppliance6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Stove" />

            </LinearLayout>
        </TableRow>


    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>